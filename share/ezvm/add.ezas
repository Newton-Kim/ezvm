.entry main
.proc main (0) 0:
	.memories 8
	mv l:0 l:1, 1 2
	add l:2, l:0 l:1
	call print (stdout l:2 "=" l:0 "+" l:1)
	add l:3, 1 2 3 4
	call print (stdout l:3 "=1+2+3+4")
	mv l:4 l:5, "hello" "world"
	add l:6, l:4 l:5
	call print (stdout l:6 "=" l:4 "+" l:5)
	add l:7, "I " "am " "Sam"
	call print (stdout l:7)
