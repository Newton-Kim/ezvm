.import io
.entry main
.proc main (0) 0:
	.memories 6
	mv l:0 l:1, 1 2
	sub l:2 l:3, l:0 l:1
	call io:print (stdout l:2 "=" l:0 "-" l:1)
	beq l:3 int_zero
	blt l:3 int_neg
	call io:print (stdout l:2 " is positive value")
	bra int_end
	:int_neg
	call io:print (stdout l:2 " is negative value")
	bra int_end
	:int_zero
	call io:print (stdout l:2 " is zero value")
	:int_end
	sub l:4 l:5, 1 2 3 4
	call io:print (stdout l:4 "=1-2-3-4")

